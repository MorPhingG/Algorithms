import java.lang.*;

public class CutRod {
	int price[] = {1, 5, 8, 9, 10, 17, 17, 20, 24, 30}; 
	
	public static void main(String[] args) {
		int n = 20;
		cut(price, n);

	}
	
	public int cut(int[] p, int n){
		int q=-10;
		if(n == 0)
			return 0;
		for (int i = 1; i < n; i++){
			q = Math.max(q, p[i]+cut(p,n-i));
		}		
		return q;
				
	}

}
